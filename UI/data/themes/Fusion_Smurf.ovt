}
QSlider::groove:horizontal {
  height: 4px;
}
QSlider::groove:vertical {
  width: 4px;
}
QSlider::add-page, QSlider::sub-page {
  background: #242424;
  border-radius: 2px;
}
QSlider::add-page:disabled, QSlider::sub-page:disabled {
  background: rgba(36, 36, 36, 127);
}
QSlider::add-page:vertical, QSlider::sub-page:horizontal {
  background: #1565c0;
}
QSlider::add-page:vertical:disabled, QSlider::sub-page:horizontal:disabled {
  background: rgba(21, 101, 192, 127);
}
QSlider::handle {
  background: #d0d0d0;
  border: 1px solid #1a1a1a;
  border-radius: 3px;
}
QSlider::handle:horizontal {
  margin: -2px 0px;
  width: 16px;
}
QSlider::handle:vertical {
  margin: 0px -2px;
  height: 16px;
}
QSlider::handle:hover {
  background: #e0e0e0;
}
QSlider::handle:disabled {
  background: #686868;
  border-color: rgba(26, 26, 26, 127);
}

OBSBasic #centralwidget QSlider::add-page:horizontal, OBSBasic #centralwidget QSlider::sub-page:vertical {
  background: #242424;
}
OBSBasic #centralwidget QSlider::add-page:horizontal:disabled, OBSBasic #centralwidget QSlider::sub-page:vertical:disabled {
  background: rgba(36, 36, 36, 127);
}

QSplitter::handle {
