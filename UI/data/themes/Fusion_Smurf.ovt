QDockWidget {
  titlebar-close-icon: url(theme:Fusion/close.svg);
  titlebar-normal-icon: url(theme:Fusion/popout.svg);
}
QDockWidget::window {
  titlebar-normal-icon: url(theme:Fusion/restore.svg);
}
QDockWidget::close-button, QDockWidget::float-button {
  border: none;
  icon-size: 10px 10px;
}

QWidget {
  color: #f8f8f8;
  selection-color: #ffffff;
  selection-background-color: #6b6b6b;
  font-size: 13px;
}
QWidget::active {
  selection-background-color: #1565c0;
}
QWidget::disabled {
  color: rgba(248, 248, 248, 127);
  selection-color: rgba(255, 255, 255, 127);
  selection-background-color: rgba(21, 101, 192, 127);
}

QAbstractItemView::item {
  background: transparent;
  min-height: 22px;
  max-height: 22px;
}
QAbstractItemView::item:alternate {
  background: #2a2a2a;
}
QAbstractItemView::item:selected {
  background: #1a1a1a;
}
QAbstractItemView::item:selected:active {
  background: #1565c0;
}
QAbstractItemView::item:hover {
  background: rgba(21, 101, 192, 84);
}
QAbstractItemView::item:hover:selected {
  background: #1565c0;
}

QDialog QAbstractItemView::item:selected:!active:!hover {
  background: #2e2e2e;
}

QAbstractScrollArea::corner {
  background: transparent;
}

QCheckBox[sourceTreeSubItem=true]::indicator {
  height: 7px;
  width: 7px;
  position: relative;
  left: 3px;
}

QDialog {
  background: #2e2e2e;
}
QDialog QListWidget {
  background: #1a1a1a;
  border-radius: 3px;
  padding: 3px;
}

QDialogButtonBox {
  dialogbuttonbox-buttons-have-icons: 0;
}

QDockWidget {
  background: #2e2e2e;
  font-weight: bold;
}
QDockWidget::title {
  background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #2e2e2e, stop: 1 #242424);
  padding: 5px;
  text-align: left;
}
QDockWidget::title:!window {
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;
}
QDockWidget::close-button, QDockWidget::float-button {
  border: 1px solid transparent;
  border-radius: 2px;
  max-height: 10px;
  max-width: 10px;
  right: 5px;
  subcontrol-origin: padding;
  subcontrol-position: right;
}
QDockWidget::close-button:hover, QDockWidget::float-button:hover {
  background: #4d4d4d;
}
QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
  background: #424242;
}
QDockWidget::float-button:closable {
  right: 25px;
}
QDockWidget > QWidget {
  background: #2e2e2e;
  border-bottom-left-radius: 3px;
  border-bottom-right-radius: 3px;
}
QDockWidget > QCefWidget {
  padding: 2px;
}
QDockWidget::window {
  border: 1px solid #224a77;
}
QDockWidget::active:window {
  border-color: #1565c0;
}
QDockWidget QToolBar QAbstractButton {
  margin-top: 3px;
  padding: 3px;
}
QDockWidget QToolBar::separator {
  width: 0px;
}
QDockWidget QAbstractScrollArea {
  background: transparent;
}

OBSDock #controlsFrame,
OBSDock #mixerFrame,
OBSDock #scenesFrame,
OBSDock #sourcesFrame,
OBSDock #transitionsFrame {
