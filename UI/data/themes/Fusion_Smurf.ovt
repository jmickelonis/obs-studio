QCheckBox::indicator,
SilentUpdateCheckBox::indicator,
OBSBasicSettings #colorsGroupBox::indicator {
  width: 18px;
  height: 18px;
}
QCheckBox::indicator:unchecked,
SilentUpdateCheckBox::indicator:unchecked,
OBSBasicSettings #colorsGroupBox::indicator:unchecked {
  image: url(theme:Fusion/ui/checkbox.svg);
}
QCheckBox::indicator:checked,
SilentUpdateCheckBox::indicator:checked,
OBSBasicSettings #colorsGroupBox::indicator:checked {
  image: url(theme:Fusion/ui/checkbox checked.svg);
}
QCheckBox::indicator:hover:!pressed:unchecked,
SilentUpdateCheckBox::indicator:hover:!pressed:unchecked,
OBSBasicSettings #colorsGroupBox::indicator:hover:!pressed:unchecked {
  image: url(theme:Fusion/ui/checkbox hover.svg);
}
QCheckBox::indicator:hover:!pressed:checked,
SilentUpdateCheckBox::indicator:hover:!pressed:checked,
OBSBasicSettings #colorsGroupBox::indicator:hover:!pressed:checked {
  image: url(theme:Fusion/ui/checkbox checked hover.svg);
}
QCheckBox::indicator:disabled:unchecked,
SilentUpdateCheckBox::indicator:disabled:unchecked,
